      }
    },
    "kakao": {
      "type": "library",
      "root": "libs/kakao",
      "entryFile": "index",
      "sourceRoot": "libs/kakao/src",
      "compilerOptions": {
        "tsConfigPath": "libs/kakao/tsconfig.lib.json"
      }
    },
    "aws": {
      "type": "library",
      "root": "libs/aws",
      "entryFile": "index",
      "sourceRoot": "libs/aws/src",
      "compilerOptions": {
        "tsConfigPath": "libs/aws/tsconfig.lib.json"
