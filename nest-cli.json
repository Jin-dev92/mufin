        "tsConfigPath": "apps/mufin-nestjs/tsconfig.app.json"
      }
    },
    "encryption": {
      "type": "library",
      "root": "libs/encryption",
      "entryFile": "index",
      "sourceRoot": "libs/encryption/src",
      "compilerOptions": {
        "tsConfigPath": "libs/encryption/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "health": {
      "type": "library",
      "root": "libs/health",
      "entryFile": "index",
